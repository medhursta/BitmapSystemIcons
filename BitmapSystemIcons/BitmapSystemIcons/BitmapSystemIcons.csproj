<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	  <PackageId>BitmapSystemIcons</PackageId>
	  <Version>1.0.0</Version>
	  <Authors>Anthony Medhurst</Authors>
	  <Company>ALM Websites</Company>
	  <PackageDescription>BitmapSystemIcon is a simple library which I create to use the Visual Studio 2022 Image Library as button icons within a WinFroms application.</PackageDescription>
	  <RepositoryUrl>https://github.com/medhursta/BitmapSystemIcons</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Svg" Version="3.4.4" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
